{
	"compilerOptions": {
		"module": "CommonJS",
		"outDir": "./build",
		"strict": true,
		"strictPropertyInitialization": false,
		"rootDirs": ["./src"],
		"esModuleInterop": true,
		"experimentalDecorators": true, 
		"emitDecoratorMetadata": true,  
		"skipLibCheck": true,
        "forceConsistentCasingInFileNames": true  
	}
}